# EditorConfig: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[*.go]
# Set default charset
charset = utf-8
# Tab indentation with specified with
indent_style = tab
tab_width = 4
